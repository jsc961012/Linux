CC =gcc
SUBDIRS =fRead \
		 list \
		 main \
		 relist \
		 search \
		 Sort \
		 rbtree \
		 obj

OBJS = readFile.o list.o relist.o Search.o quickSort.o test2.o
BIN = myapp
OBJS_DIR = obj
BIN_DIR = bin
export CC OBJS BIN OBJS_DIR BIN_DIR

all: CHECK_DIR $(SUBDIRS)
CHECK_DIR:
	mkdir -p $(BIN_DIR)
$(SUBDIRS) : ECHO
	make -C $@
ECHO:
	@echo $(SUBDIRS)
	@echo begin complie
CLEAN:
	@$(RM) $(OBJS_DIR)/*.o
	@rm -rf $(BIN_DIR)
